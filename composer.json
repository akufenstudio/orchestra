{
    "name": "akufen/orchestra",
    "type": "library",
    "description": "A minimalist object-oriented superset for WordPress using Symfony.",
    "keywords": ["symfony", "doctrine", "wordpress", "mvc", "orm"],
    "homepage": "https://github.com/akufenstudio/orchestra",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Charbonneau",
            "email": "nicholas@akufen.ca"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "doctrine/orm": "^2.5",
        "symfony/routing": "^3.1",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/http-foundation": "^3.1",
        "symfony/templating": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Akufen\\Orchestra\\": "src/Orchestra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akufen\\Orchestra\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "clean": [
            "vendor/bin/phpcbf"
        ],
        "tests": [
            "vendor/bin/phpcs",
            "vendor/bin/phpunit"
        ]
    }
}
