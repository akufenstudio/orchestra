{
    "name": "akufen/orchestra",
    "type": "library",
    "description": "A minimalist object-oriented superset for WordPress using Symfony.",
    "keywords": ["symfony", "wordpress", "mvc", "orm"],
    "homepage": "https://github.com/akufenstudio/orchestra",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Charbonneau",
            "email": "nicholas@akufen.ca"
        },
        {
            "name": "Akufen Atelier Creatif",
            "email": "info@akufen.ca",
            "homepage": "http://akufen.ca/"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/routing": "^3.1",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/http-foundation": "^3.1",
        "symfony/class-loader": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.6",
        "techpivot/phalcon-ci-installer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Akufen\\Orchestra\\": "src/Orchestra"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akufen\\Orchestra\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "clean": [
            "vendor/bin/phpcbf"
        ],
        "tests": [
            "vendor/bin/phpcs",
            "vendor/bin/phpunit"
        ]
    }
}
